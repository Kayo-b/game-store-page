: Math.round((discount + count) * 100 / 100).toFixed(2)\n            \n        }\n        return returningValues;\n    }\n   \n    // console.log(title)\n    // setcartContent(title, price, saving)\n\n    function RemoveFromCard(item) {\n        \n        let newShopCart = shopCart\n        newShopCart.splice(shopCart.indexOf(item), 1)\n\n        setCart([...shopCart])\n    }\n\n    function AddQuantity(item) {\n        let newShopCart = shopCart;\n        newShopCart[shopCart.indexOf(item)].quantity += 1;\n\n        setCart([...shopCart])\n\n    }\n\n    function SubtractQuantity(item) {\n        let newShopCart = shopCart;\n        newShopCart[shopCart.indexOf(item)].quantity -= 1;\n        if(newShopCart[shopCart.indexOf(item)].quantity === 0) {\n            RemoveFromCard(item)\n        }\n        setCart([...shopCart])\n\n    }\n\n    function SetQuantity(value, item) {\n        let newShopCart = shopCart;\n        newShopCart[shopCart.indexOf(item)].quantity = value;\n        if(value === \"0\" || value === \"\") {\n            RemoveFromCard(item)\n        }\n        setCart([...shopCart])\n    }\n    return(\n        <div className='shopCartContaienr'>\n            <div className='shopCart' style={{display: cartDisplay}}>\n                <button onClick={() => setCartDisplay('none')}>X</button>\n                <h3>Shopping Cart</h3>\n                <p>This is a page under construction</p>\n                <ul className=\"cartList\">{shopCart.map(item => \n                    <li key={item.dealID + shopCart.indexOf(item)}>\n\n                    <button onClick={() => RemoveFromCard(item)}>x</button>{item.title}\n                    <button onClick={() => AddQuantity(item)}>+</button>\n                    <input className=\"amoutInput\" value={item.quantity} onChange={(e) => SetQuantity(e.target.value, item)}></input>\n                    <button onClick={() => SubtractQuantity(item)}>-</button>\n                    </li>\n                    )}\n                </ul>\n                <p>Price without discount: ${calculateSubtotal().noDiscount} USD</p>\n                <p>Discount: ${calculateSubtotal().totalDiscount} USD</p>\n                <p>Subtotal: ${calculateSubtotal().subtotal} USD</p>\n            </div>\n        </div>\n    )\n}\nexport default Cart\n","/home/kayo-b/the_odin_project/game-store-page/src/components/Sidebar.js",[],[],{"ruleId":"84","severity":1,"message":"85","line":4,"column":8,"nodeType":"86","messageId":"87","endLine":4,"endColumn":11},{"ruleId":"84","severity":1,"message":"88","line":10,"column":8,"nodeType":"86","messageId":"87","endLine":10,"endColumn":15},{"ruleId":"84","severity":1,"message":"89","line":11,"column":10,"nodeType":"86","messageId":"87","endLine":11,"endColumn":19},{"ruleId":"84","severity":1,"message":"90","line":18,"column":18,"nodeType":"86","messageId":"87","endLine":18,"endColumn":25},{"ruleId":"84","severity":1,"message":"91","line":22,"column":21,"nodeType":"86","messageId":"87","endLine":22,"endColumn":31},{"ruleId":"92","severity":1,"message":"93","line":46,"column":8,"nodeType":"94","endLine":46,"endColumn":10,"suggestions":"95"},{"ruleId":"84","severity":1,"message":"96","line":1,"column":8,"nodeType":"86","messageId":"87","endLine":1,"endColumn":12},{"ruleId":"92","severity":1,"message":"97","line":9,"column":7,"nodeType":"94","endLine":9,"endColumn":9,"suggestions":"98"},{"ruleId":"84","severity":1,"message":"99","line":1,"column":17,"nodeType":"86","messageId":"87","endLine":1,"endColumn":25},{"ruleId":"84","severity":1,"message":"100","line":3,"column":8,"nodeType":"86","messageId":"87","endLine":3,"endColumn":19},{"ruleId":"84","severity":1,"message":"101","line":6,"column":12,"nodeType":"86","messageId":"87","endLine":6,"endColumn":26},{"ruleId":"84","severity":1,"message":"102","line":6,"column":28,"nodeType":"86","messageId":"87","endLine":6,"endColumn":45},{"ruleId":"84","severity":1,"message":"103","line":8,"column":11,"nodeType":"86","messageId":"87